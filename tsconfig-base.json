{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "allowJs": true,
    "composite": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "exclude": [
    "../build/**/*.ts"
  ]
}
